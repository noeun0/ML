    "import cv2\n",
    "import numpy as np\n",
    "img = np.full(shape=(400,400,3), fill_value=255, dtype=np.uint8)\n",
    "cv2.line(img, (50,50), (200,50), color=(0,0,255), thickness=10)\n",
    "\n",
    "cv2.imshow(\"img\",img)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
